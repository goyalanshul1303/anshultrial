<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="WelcomeErrorAppearance" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/login_email_button</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="TextAppearence.App.TextInputLayout" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/theme_accent</item>
        <item name="android:textSize">14sp</item>
        <item name="colorControlNormal">@color/resume_btn_color</item>

    </style>
    <style name="Theme.MyFancyTheme" parent="android:Theme.Holo">
        <item name="android:homeAsUpIndicator">@drawable/back</item>
    </style>

    <style name="btnActionStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentBottom">true</item>
        <item name="android:background">@color/button_bg_blue</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">14sp</item>
    </style>
</resources>